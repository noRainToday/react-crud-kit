{
  "name": "react-crud-lib",
  "version": "1.0.0",
  "description": "A React CRUD library with playground for testing",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter playground dev",
    "build": "pnpm --filter react-crud-kit build",
