{
  "name": "react-crud-lib",
  "version": "1.0.0",
  "description": "A React CRUD library with playground for testing",
  "private": true,
  "scripts": {
    "build": "pnpm --filter react-crud-kit build",
    "dev:playground": "pnpm --filter playground dev",
    "dev:kit": "pnpm --filter react-crud-kit dev",
    "dev:all": "concurrently \"pnpm --filter playground dev\" \"pnpm --filter react-crud-kit dev\"",
    "build:all": "pnpm -r build",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean",
    "install:all": "pnpm install",
    "test": "vitest",
    "test:watch": "vitest"
  },
  "keywords": [
    "react",
    "crud",
    "library",
    "monorepo"
  ],
  "author": "test",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "typescript": "~5.8.3"
  }
}